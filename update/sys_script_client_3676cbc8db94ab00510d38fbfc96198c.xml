        <description>Script to get the actual date when the user register a Siniestro</description>
        <field>u_numero_factura</field>
        <global>true</global>
        <messages/>
        <name>Set Actual Date</name>
        <order/>
        <script><![CDATA[function onLoad() {
	var ga = new GlideAjax('getActualDate'); 
	ga.addParam('sysparm_name','actualdate'); 
	ga.getXML(HelloWordParse);

	//Generate the Request to get the response
	function HelloWordParse(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");
		g_form.setValue('u_fecha_reporte',answer); 
		g_form.setReadOnly('u_fecha_reporte',true);
	}
	
}]]></script>
